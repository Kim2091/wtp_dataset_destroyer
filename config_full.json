{
  "input": "/media/umzi/H/датасеты/descreenton/HR",
  "output": "/media/umzi/H/датасеты/descreenton/HR2",
  "process":
    {
      "screenton": {
        "gray_or_color": true,
        "color": {
          "dot_size": 7,
          "r":[0,7] ,
          "g":[0,7] ,
          "b":[0,7] },
        "dot_size": 7,
        "lqhq": true
      },
      "resize": {
        "alg_lq": ["nearest", "linear", "catrom", "bspline", "mitchell", "lanczos", "gauss", "down_up", "down_down"],
        "alg_hq": ["catrom"],
        "down_up": {
          "up": [1, 3],
          "alg_up": ["nearest", "linear", "catrom", "bspline", "mitchell", "lanczos", "gauss"],
          "alg_down": ["linear", "catrom", "bspline", "mitchell", "lanczos", "gauss", "down_down"]
        },
        "down_down": {
          "step": 15,
          "alg_down": ["linear", "catrom", "bspline", "mitchell", "gauss"]
        },
        "rand_scale": [1, 2, 0.25],
        "scale": 4
      },
      "blur": {
        "method": ["box", "gauss", "box", "median"],
        "kernel": [0, 6, 1],
        "median_kernel": [0,3,1]
      },
      "halo": {
        "sharpening_factor": [0, 2],
        "kernel_median": [1, 3],
        "box_kernel": [1, 3],
        "laplacian": [1]
      },
      "noise": {
        "rand": [0, 0.3]
      },
      "color_loss": {
        "high": [240, 255],
        "low": [0, 10],
        "gamma": [0.8, 1.2]
      },
      "sin": {

        "shape": [100,1000,100],
        "alpha": [0.1,0.5],
        "bias": [0.8,1.2],
        "vertical": [true,false]
      },
      "compress": {
        "compress_compress": [1, 5],
        "algorithm": ["jpeg", "webp"],
        "comp": [40, 100]
      }
    }
  ,
  "tile": {
    "size": 512,
    "no_wb": true
  },
  "gray_or_color": true
}